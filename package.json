{
  "name": "winston-dev-tool",
  "version": "1.0.0",
  "description": "Preview your logs in the browser",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "dev:serve": "better-npm-run dev:serve",
    "dev:client": "better-npm-run dev:client",
    "dev": "concurrently \"npm run dev:serve\" \"npm run dev:client\"",
    "dev:log": "better-npm-run dev:log"
  },
  "betterScripts": {
    "dev:serve": {
      "command": "nodemon server",
      "env": {
        "NODE_ENV": "dev",
        "PORT": 5050,
        "HOST": "localhost"
      }
    },
    "dev:client": {
      "command": "webpack-dev-server --config config/webpack.config.js",
      "env": {
        "NODE_ENV": "dev",
        "SERVER_PORT": 5050,
        "SERVER_HOST": "localhost"
      }
    },
    "dev:log": {
      "command": "node dev/sendDevLogs",
      "env": {
        "SERVER_HOST": "localhost",
        "SERVER_PORT": 5050
      }
    }
  },
  "author": "antoinechalifour <antoine.chalifour@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "better-npm-run": "0.0.8",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "faker": "^3.1.0",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^1.3.0",
    "standard": "^7.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "winston": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "ip": "^1.1.3",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "reset.css": "^2.0.2",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  }
}
